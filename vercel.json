
{
    "version": 2,
    "rewrites": [
        { "source": "/(.*)", "destination": "/" }
      ],
    "builds": [
     {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["client/**"] }
     }
    ],
    "routes": [
     {
      "src": "/(.*)",
      "dest": "index.js"
     }
    ]
   }